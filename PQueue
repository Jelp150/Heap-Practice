#include <cmath>
#include <cstdio>
#include <queue>
#include <vector>
#include <iostream>
#include <algorithm>
#include <stack>
using namespace std;

int main()
{
    int queries;
    int command;
    int value;
    priority_queue<int,vector<int>,greater<int>> q1;
    cin >> queries;
    while (queries != 0)
    {
        cin >> command;
        if (command == 1)
        {
            cin >> value;
            q1.push(value);

        }
        else if (command == 2)
        {
            cin >> value;
            priority_queue<int,vector<int>,greater<int>> temp = q1;
            vector<int> v_temp;
            while (!temp.empty())
            {
                v_temp.push_back(temp.top());
                temp.pop();
            }
            bool found = false;
            int i = 0;
            while (found != true)
            
            {
                if (v_temp[i] == value)
                {
                    v_temp.erase(v_temp.begin() + i);   
                    found = true;
                }
                i++;
            
            }
            for (int k = 0; k < v_temp.size(); k++)
            {
                temp.push(v_temp[k]);
            }
            q1 = temp;
           
        }
        else
        {
            
            cout << q1.top() <<endl;
        }
        queries--;
    }
    return 0;
}

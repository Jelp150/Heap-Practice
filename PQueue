#include <cmath>
#include <cstdio>
#include <queue>
#include <vector>
#include <iostream>
#include <algorithm>

using namespace std;

int main()
{
    int queries;
    int command;
    int value;
    priority_queue<int> q1;
    cin >> queries;
    while (queries != 0)
    {
        cin >> command;

        if (command == 1)
        {
            cin >> value;
            q1.push(value);

        }
        else if (command == 2)
        {
            q1.pop(); 
        }
        else
        {
            cout << value <<endl;
        }
        queries--;
    }
    return 0;
}
